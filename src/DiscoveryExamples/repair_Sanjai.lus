node T_monitor(in: int; out: int) returns (ok: bool);
let
	ok = in > 0 => (out > in + 5 and out < in + 10);
tel;

node R_wrapper(in: int) returns (out: int);
var
	st : int;
let
	(st, out) = R_impl(in, 1 -> pre st);
tel;

node R_impl(in, preS: int) returns (postS, out: int);
let
		postS = preS + if in > 10 then 1 else 0;
		out =  in + 5 + postS;
tel;

node main(in: int) returns (ok: bool);
let
	ok = T_monitor(in, R_wrapper(in));

	--%PROPERTY ok;
tel;
