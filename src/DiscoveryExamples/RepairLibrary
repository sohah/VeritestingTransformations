
--This is the repair library of nodes that could be used in the repair.



node gather_1_Wrapper(e1:int; e2: int; flag:operation; hole1:int; hole2:int) returns (val:int);

let
	val = gather_1(e1, e2, flag, hole1, hole2);


node gather_1(e1:int; e2: int; flag:operation; hole1:int; hole2:int) returns (val:int);

let
	val = (e1 * hole1) + (e2 * hole2);
tel;



